{
    "name": "azine/mailgunwebhooks-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Bundle to easily capture feedback from mailgun.com via their provided webhooks",
    "keywords": ["Mailgun", "mailgun.com", "transactional email", "email", "webhooks"],
    "homepage": "https://github.com/azine/AzineMailgunWebhooksBundle.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Azine IT Services AG",
            "email": "github@azine-it.ch"
        }
    ],
    "config": {
        "process-timeout": 590,
        "github-protocols": ["https", "git", "ssh"],
        "github-domains": ["github.com"]
        },
    "require": {
        "php": ">=7.0",
        "ext-mailparse": "*",
        "symfony/framework-bundle": "^4.0|^5.4",
        "symfony/config": "^4.0|^5.4",
        "symfony/console": "^4.0|^5.4",
        "symfony/dependency-injection": "^4.0|^5.4",
        "symfony/dom-crawler": "^4.0|^5.4",
        "symfony/event-dispatcher": "^4.0|^5.4",
        "symfony/http-foundation": "^4.0|^5.4",
        "symfony/http-kernel": ">=4.4.50|^5.4",
        "symfony/yaml": "^4.0|^5.4",
        "symfony/process": "^4.0|^5.4",
        "symfony/routing": "^4.0|^5.4",
        "symfony/translation": "^4.0|^5.4",
        "twig/twig": ">=1.34",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/common": "~2.2,>=2.2.3"
    },
    "require-dev": {
        "phpunit/phpunit":  "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-0": { "Azine\\MailgunWebhooksBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Azine/MailgunWebhooksBundle"
}
